{
  "buildCommand":"cd frontend && npm install && npm run build",
  "outputDirectory":"client/dist",
  "framework":"vite",
  "version": 2,
  "builds": [
      {
          "src": "index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "index.js"
      }
  ],
  "rewrites":[
    {
      "source":"/(.*)",
      "destination":"index.js"
    }
  ]
}