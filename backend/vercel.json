{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version" }
      ]
    }
  ],
  "version": 2,
  "builds": [
      {
          "src": "index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "index.js"
      },
      {
        "src": "/product",
        "dest": "index.js"
      },
      {
        "src": "/product/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/category",
        "dest": "index.js"
      },
      {
        "src": "/category/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/brand",
        "dest": "index.js"
      },
      {
        "src": "/brand/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/user",
        "dest": "index.js"
      },
      {
        "src": "/user/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/auth",
        "dest": "index.js"
      },
      {
        "src": "/auth/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/cart",
        "dest": "index.js"
      },
      {
        "src": "/cart/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/orders",
        "dest": "index.js"
      },
      {
        "src": "/orders/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/webhook",
        "dest": "index.js"
      },
      {
        "src": "/create-payment-intent",
        "dest": "index.js"
      },
      {
        "src": "/",
        "dest": "dest/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "dest/$1"
      }
  ]
}